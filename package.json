{
  "name": "rilo-inventory-server",
  "version": "1.0.0",
  "description": "Rilo Inventory",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "node dist/server.ts",
    "build": "tsc",
    "lint:check": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "lint:fix": "lint . --fix",
    "format:check": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "format:fix": "prettier --write .",
    "lint-prettier": "npm run lint:check && npm run format:check",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "ISC",
  "lint-staged": {
    "src/**/*.ts": "npm run lint-prettier"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.0",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-spawn": "^7.0.3",
    "dotenv": "^16.2.0",
    "express": "^4.18.2",
    "http-status": "^1.6.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.3.0",
    "mongoose-unique-validator": "^4.0.0",
    "validator": "^13.9.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/mongoose-unique-validator": "^1.0.7",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  }
}
